<component name="threads" type="synapse/template">
    <subComponents>
        <component name="listAllThreads">
            <file>listAllThreads.xml</file>
            <description>List all threads</description>
        </component>
        <component name="getAThread">
            <file>getAThread.xml</file>
            <description>get an email thread</description>
        </component>
        <component name="trashThreads">
            <file>trashThreads.xml</file>
            <description>Send threads to trash</description>
        </component>
        <component name="unTrashThreads">
            <file>unTrashThreads.xml</file>
            <description>Removed threads from trash</description>
        </component>
        <component name="modifyExistingThreads">
            <file>modifyExistingThreads.xml</file>
            <description>Modify existing threads</description>
        </component>
        <component name="deleteThreads">
            <file>deleteThreads.xml</file>
            <description>delete an existing thread</description>
        </component>
    </subComponents>
</component>