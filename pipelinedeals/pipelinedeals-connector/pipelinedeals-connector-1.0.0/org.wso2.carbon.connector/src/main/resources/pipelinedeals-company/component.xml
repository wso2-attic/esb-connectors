<component name="pipelinedeals-company" type="synapse/template">
   <subComponents>
      <component name="createCompany">
         <file>createCompany.xml</file>
         <description>Create a new company in PipelineDeals using the provided Parameters.</description>
      </component>
      <component name="updateCompany">
         <file>updateCompany.xml</file>
         <description>Update details of the company identified by the provided company ID.</description>
      </component>
      <component name="getCompany">
         <file>getCompany.xml</file>
         <description>Retrieve details of a company identified by the provided company ID.</description>
      </component>
      <component name="listCompanies">
         <file>listCompanies.xml</file>
         <description>Retrieve a list of companies from PipelineDeals.</description>
      </component>
   </subComponents>
</component>
