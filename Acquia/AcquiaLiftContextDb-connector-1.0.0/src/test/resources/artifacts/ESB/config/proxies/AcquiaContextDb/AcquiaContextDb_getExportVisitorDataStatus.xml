<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="AcquiaContextDb_getExportVisitorDataStatus"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
<target>
    <inSequence>
        <property name="secretKey" expression="json-eval($.secretKey)"/>
        <property name="accessKey" expression="json-eval($.accessKey)"/>
        <property name="accountId" expression="json-eval($.accountId)"/>
        <property name="statusId" expression="json-eval($.statusId)"/>
        <AcquiaContextDb.init>
            <secretKey>{$ctx:secretKey}</secretKey>
            <accessKey>{$ctx:accessKey}</accessKey>
        </AcquiaContextDb.init>
        <AcquiaContextDb.getExportVisitorDataStatus>
            <accountId>{$ctx:accountId}</accountId>
            <statusId>{$ctx:statusId}</statusId>
        </AcquiaContextDb.getExportVisitorDataStatus>
        <respond/>
    </inSequence>
    <outSequence>
        <log/>
        <send/>
    </outSequence>
</target>
<description/>
</proxy>
