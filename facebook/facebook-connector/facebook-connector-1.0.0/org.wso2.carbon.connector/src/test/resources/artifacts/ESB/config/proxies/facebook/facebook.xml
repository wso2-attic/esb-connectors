                    <facebook.updateEvent>
                        <eventId>{$ctx:eventId}</eventId>
                        <name>{$ctx:name}</name>
                        <description>{$ctx:description}</description>
                    </facebook.updateEvent>
                </case>
                <case regex="urn:createPostOnEventWall">
                    <facebook.createPostOnEventWall>
                        <eventId>{$ctx:eventId}</eventId>
                        <message>{$ctx:message}</message>
                        <link>{$ctx:link}</link>
                        <caption>{$ctx:caption}</caption>
                    </facebook.createPostOnEventWall>
                </case>
                <case regex="urn:deleteEventProfilePicture">
                    <facebook.deleteEventProfilePicture>
                        <eventId>{$ctx:eventId}</eventId>
                    </facebook.deleteEventProfilePicture>
                </case>
                <case regex="urn:deleteEvent">
                    <facebook.deleteEvent>
                        <eventId>{$ctx:eventId}</eventId>
                    </facebook.deleteEvent>
                </case>
                <case regex="urn:getVideoDetails">
                    <facebook.getVideoDetails>
                        <videoId>{$ctx:videoId}</videoId>
                        <fields>{$ctx:fields}</fields>
                        <connection>{$ctx:connection}</connection>
                    </facebook.getVideoDetails>
                </case>
                <case regex="urn:createComment">
                    <facebook.createComment>
                        <resourceId>{$ctx:resourceId}</resourceId>
                        <message>{$ctx:message}</message>
                    </facebook.createComment>
                </case>
                <case regex="urn:updateComment">
                    <facebook.updateComment>
                        <commentId>{$ctx:commentId}</commentId>
                        <message>{$ctx:message}</message>
                        <isHidden>{$ctx:isHidden}</isHidden>
                    </facebook.updateComment>
                </case>
                <case regex="urn:createLike">
                    <facebook.createLike>
                        <resourceId>{$ctx:resourceId}</resourceId>
                    </facebook.createLike>
                </case>
                <case regex="urn:deleteLike">
                    <facebook.deleteLike>
                        <resourceId>{$ctx:resourceId}</resourceId>
                    </facebook.deleteLike>
                </case>
                <case regex="urn:getCommentDetails">
