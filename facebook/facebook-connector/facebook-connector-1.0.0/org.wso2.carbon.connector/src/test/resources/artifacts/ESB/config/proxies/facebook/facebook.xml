                        <ticketUri>{$ctx:ticketUri}</ticketUri>
                        <noFeedStory>{$ctx:noFeedStory}</noFeedStory>
                    </facebook.createPageEvent>
                </case>
                <case regex="urn:publishPagePost">
                    <facebook.publishPagePost>
                        <pageId>{$ctx:pageId}</pageId>
                        <message>{$ctx:message}</message>
                        <link>{$ctx:link}</link>
                        <picture>{$ctx:picture}</picture>
                        <name>{$ctx:name}</name>
                        <caption>{$ctx:caption}</caption>
                        <description>{$ctx:description}</description>
                        <actions>{$ctx:actions}</actions>
                        <place>{$ctx:place}</place>
                        <tags>{$ctx:tags}</tags>
                        <objectAttachment>{$ctx:objectAttachment}</objectAttachment>
                        <targeting>{$ctx:targeting}</targeting>
                        <feedTargeting>{$ctx:feedTargeting}</feedTargeting>
                        <published>{$ctx:published}</published>
                        <scheduledPublishTime>{$ctx:scheduledPublishTime}</scheduledPublishTime>
                        <backdatedTime>{$ctx:backdatedTime}</backdatedTime>
                        <backdatedTimeGranularity>{$ctx:backdatedTimeGranularity}</backdatedTimeGranularity>
                        <childAttachments>{$ctx:childAttachments}</childAttachments>
                        <multiShareOptimized>{$ctx:multiShareOptimized}</multiShareOptimized>
                        <multiShareEndCard>{$ctx:multiShareEndCard}</multiShareEndCard>
                    </facebook.publishPagePost>
                </case>
                <case regex="urn:updatePageDetails">
                    <facebook.updatePageDetails>
                        <pageId>{$ctx:pageId}</pageId>
                        <name>{$ctx:name}</name>
                        <about>{$ctx:about}</about>
                        <hours>{$ctx:hours}</hours>
