            <facebook.init>
                <apiUrl>{$ctx:apiUrl}</apiUrl>
                <apiVersion>{$ctx:apiVersion}</apiVersion>
                <accessToken>{$ctx:accessToken}</accessToken>
            </facebook.init>
            <switch source="get-property('transport', 'Action')">
                <case regex="urn:getEventDetails">
                    <facebook.getEventDetails>
                        <eventId>{$ctx:eventId}</eventId>
                        <fields>{$ctx:fields}</fields>
                    </facebook.getEventDetails>
                </case>
                <case regex="urn:updateEventTicketURI">
                    <facebook.updateEventTicketURI>
                        <eventId>{$ctx:eventId}</eventId>
                        <ticketUri>{$ctx:ticketUri}</ticketUri>
                    </facebook.updateEventTicketURI>
                </case>
                <case regex="urn:createAttendingRSVP">
                    <facebook.createAttendingRSVP>
                        <eventId>{$ctx:eventId}</eventId>
                    </facebook.createAttendingRSVP>
                </case>
                <case regex="urn:createDeclinedRSVP">
                    <facebook.createDeclinedRSVP>
                        <eventId>{$ctx:eventId}</eventId>
                    </facebook.createDeclinedRSVP>
                </case>
                <case regex="urn:createMaybeRSVP">
                    <facebook.createMaybeRSVP>
                        <eventId>{$ctx:eventId}</eventId>
                    </facebook.createMaybeRSVP>
                </case>
                <case regex="urn:createEventInvitation">
                    <facebook.createEventInvitation>
                        <eventId>{$ctx:eventId}</eventId>
                        <userIds>{$ctx:userIds}</userIds>
                    </facebook.createEventInvitation>
                </case>
                <case regex="urn:deleteEventInvitation">
                    <facebook.deleteEventInvitation>
                        <eventId>{$ctx:eventId}</eventId>
                        <userId>{$ctx:userId}</userId>
                    </facebook.deleteEventInvitation>
                </case>
