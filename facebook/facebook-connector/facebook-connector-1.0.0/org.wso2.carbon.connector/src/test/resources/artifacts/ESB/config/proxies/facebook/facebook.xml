                        <authReferralUserPerms>{$ctx:authReferralUserPerms}</authReferralUserPerms>
                        <authReferralResponseType>{$ctx:authReferralResponseType}</authReferralResponseType>
                        <canvasFluidHeight>{$ctx:canvasFluidHeight}</canvasFluidHeight>
                        <canvasFluidWidth>{$ctx:canvasFluidWidth}</canvasFluidWidth>
                        <canvasUrl>{$ctx:canvasUrl}</canvasUrl>
                        <contactEmail>{$ctx:contactEmail}</contactEmail>
