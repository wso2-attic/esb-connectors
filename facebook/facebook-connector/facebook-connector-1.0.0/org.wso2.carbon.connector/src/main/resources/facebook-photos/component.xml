<component name="facebook-photos" type="synapse/template">
  <subComponents>
    <component name="getPhotoDetails">
      <file>getPhotoDetails.xml</file>
      <description>Get details of a photo published to Facebook by photo ID.</description>
    </component>
    <component name="createPhotoTag">
      <file>createPhotoTag.xml</file>
      <description>Adds a new tag to a photo.</description>
    </component>
    <component name="deletePhoto">
      <file>deletePhoto.xml</file>
      <description>Delete a photo.</description>
    </component>
  </subComponents>
</component>