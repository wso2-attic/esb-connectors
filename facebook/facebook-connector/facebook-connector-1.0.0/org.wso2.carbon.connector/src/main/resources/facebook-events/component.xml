<component name="facebook-events" type="synapse/template">
  <subComponents>
    <component name="getEventDetails">
      <file>getEventDetails.xml</file>
      <description>Get details of event by ID.</description>
    </component>
    <component name="createAttendingRSVP">
      <file>createAttendingRSVP.xml</file>
      <description>RSVP a user as attending an event.</description>
    </component>
    <component name="createDeclinedRSVP">
      <file>createDeclinedRSVP.xml</file>
      <description>RSVP a user as declining an event.</description>
    </component>
    <component name="createMaybeRSVP">
      <file>createMaybeRSVP.xml</file>
      <description>RSVP a user as maybe for an event.</description>
    </component>
    <component name="postEventPhotos">
      <file>postEventPhotos.xml</file>
      <description>Post photos to an event.</description>
    </component>
    <component name="createPostOnEventWall">
      <file>createPostOnEventWall.xml</file>
      <description>Create a post on the event wall.</description>
    </component>
  </subComponents>
</component>