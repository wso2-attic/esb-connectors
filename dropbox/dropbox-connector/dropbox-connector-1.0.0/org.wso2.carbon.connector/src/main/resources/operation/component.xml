<component name="operation" type="synapse/template">
    <subComponents>
        <component name="copy">
            <file>copy.xml</file>
            <description>Copies a file or folder to a new location.</description>
        </component>
        <component name="getFile">
            <file>getFile.xml</file>
            <description>Get file from dropbox.</description>
        </component>
        <component name="createFolder">
            <file>createFolder.xml</file>
            <description>Creates a folder.</description>
        </component>
        <component name="delete">
            <file>delete.xml</file>
            <description>Delete folder.</description>
        </component>
        <component name="move">
            <file>move.xml</file>
            <description>Moves a file or folder to a new location.</description>
        </component>
        <component name="chunkUpload">
            <file>chunkUpload.xml</file>
            <description>Uploads large files to Dropbox in multiple chunks.</description>
        </component>
        <component name="commitChunk">
            <file>commitChunk.xml</file>
            <description>Completes an upload.</description>
        </component>
        <component name="restoreFile">
            <file>restoreFile.xml</file>
            <description>Restores a file path to a previous revision.</description>
        </component>
        <component name="share">
            <file>share.xml</file>
            <description>Returns a link directly to a file.</description>
        </component>
        <component name="uploadFile">
            <file>uploadFile.xml</file>
            <description>Upload file to dropbox.</description>
        </component>
        <component name="createDirectLink">
            <file>createDirectLink.xml</file>
            <description>Returns a link directly to a file.</description>
        </component>
    </subComponents>
</component>