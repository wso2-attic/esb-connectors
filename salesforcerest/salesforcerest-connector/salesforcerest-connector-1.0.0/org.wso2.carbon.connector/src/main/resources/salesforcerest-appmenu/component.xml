<component name="salesforcerest-appmenu" type="synapse/template">
    <subComponents>
        <component name="listItemsInMenu">
            <file>listItemsInMenu.xml</file>
            <description>Returns a list of items in either the Salesforce app drop-down menu or the
                Salesforce1 navigation menu.
            </description>
        </component>
        <component name="tabs">
            <file>tabs.xml</file>
            <description>Returns a list of all tabs.</description>
        </component>
        <component name="themes">
            <file>themes.xml</file>
            <description>Gets the list of icons and colors used by themes in the Salesforce
                application.
            </description>
        </component>
    </subComponents>
</component>