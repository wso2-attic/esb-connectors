<component name="fhir-history" type="synapse/template">
    <subComponents>
        <component name="history">
            <file>history.xml</file>
            <description>Retrieves the history of a particular resource supported by the system.
            </description>
        </component>
        <component name="historyAll">
            <file>historyAll.xml</file>
            <description>Retrieves the history of all resources supported by the system.
            </description>
        </component>
        <component name="historyType">
            <file>historyType.xml</file>
            <description>Retrieves the history of all resources of a given type supported by the
                system.
            </description>
        </component>
    </subComponents>
</component>