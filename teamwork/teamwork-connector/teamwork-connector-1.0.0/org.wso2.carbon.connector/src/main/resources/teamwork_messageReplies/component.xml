<component name="teamwork_messageReplies" type="synapse/template">
    <subComponents>
        <component name="updateMessageReply">
            <file>updateMessageReply.xml</file>
            <description>Modifies a single message reply.</description>
        </component>
        <component name="deleteMessageReply">
            <file>deleteMessageReply.xml</file>
            <description>Delete a message reply.</description>
        </component>
        <component name="getMessageReply">
            <file>getMessageReply.xml</file>
            <description>Returns a single message reply identified by its ID.</description>
        </component>
        <component name="getRepliesToMessage">
            <file>getRepliesToMessage.xml</file>
            <description>Retrieves all Replies to a Message.</description>
        </component>
    </subComponents>
</component>