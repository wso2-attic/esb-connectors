<component name="stripe_fileUploads" type="synapse/template">
   <subComponents>
      <component name="createAFileUpload">
         <file>createAFileUpload.xml</file>
         <description>Uploads a file to Stripe</description>
      </component>
      <component name="retrieveAFileUpload">
         <file>retrieveAFileUpload.xml</file>
         <description>Retrieves the details of an existing file object</description>
      </component>
      <component name="getListOfAllFileUploads">
         <file>getListOfAllFileUploads.xml</file>
         <description>Returns a list of the files that you have uploaded to Stripe</description>
      </component>
   </subComponents>
</component>



