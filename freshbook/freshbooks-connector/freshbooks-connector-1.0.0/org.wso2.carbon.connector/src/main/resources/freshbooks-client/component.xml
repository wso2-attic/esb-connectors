<component name="freshbooks-client" type="synapse/template">
   <subComponents>
      <component name="createClient">
         <file>createClient.xml</file>
         <description>Create a new client.</description>
      </component>
      <component name="updateClient">
         <file>updateClient.xml</file>
         <description>Update the details of the client.</description>
      </component>
      <component name="getClient">
         <file>getClient.xml</file>
         <description>Return the client details.</description>
      </component>
      <component name="deleteClient">
         <file>deleteClient.xml</file>
         <description>Delete the client.</description>
      </component>
      <component name="listClients">
         <file>listClients.xml</file>
         <description>Returns a list of client summaries.</description>
      </component>
   </subComponents>
</component>