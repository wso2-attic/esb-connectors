<component name="wunderlist-list" type="synapse/template">
   <subComponents>
      <component name="createList">
         <file>createList.xml</file>
         <description>Create a new list in WunderList account using the provided Parameters.</description>
      </component>
      <component name="updateList">
         <file>updateList.xml</file>
         <description>Update details of the list identified by the provided list ID.</description>
      </component>
      <component name="getList">
         <file>getList.xml</file>
         <description>Retrieve details of a list identified by the provided list ID.</description>
      </component>
      <component name="listLists">
         <file>listLists.xml</file>
         <description>Retrieve a List of lists from WunderList account.</description>
      </component>
   </subComponents>
</component>
