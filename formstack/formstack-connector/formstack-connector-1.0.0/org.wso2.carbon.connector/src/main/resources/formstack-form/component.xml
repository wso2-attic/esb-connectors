<component name="formstack-form" type="synapse/template">
   <subComponents>
      <component name="copyForm">
         <file>copyForm.xml</file>
         <description>Create a form using an existing form as template.</description>
      </component>
      <component name="getForm">
         <file>getForm.xml</file>
         <description>Retrieve details of a form identified by the provided form ID.</description>
      </component>
      <component name="listForms">
         <file>listForms.xml</file>
         <description>Retrieve a List of forms from Formstack account.</description>
      </component>
            <component name="deleteForm">
         <file>deleteForm.xml</file>
         <description>Delete the form from Formstack account.</description>
      </component>
      
   </subComponents>
</component>