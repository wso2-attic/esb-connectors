<component name="instagram-likes" type="synapse/template">
    <subComponents>
        <component name="getMediaLike">
            <file>getMediaLike.xml</file>
            <description> Get a list of users who have liked this media.</description>
        </component>
        <component name="setMediaLike">
            <file>setMediaLike.xml</file>
            <description> Set a like on this media by the currently authenticated user.</description>
        </component>
        <component name="deleteMediaLike">
            <file>deleteMediaLike.xml</file>
            <description>Remove a like on this media by the currently authenticated user.</description>
        </component>
    </subComponents>
</component>
