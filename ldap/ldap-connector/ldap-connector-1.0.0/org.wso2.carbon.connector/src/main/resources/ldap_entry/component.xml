<component name="ldap_entry" type="synapse/template" >
    <subComponents>
        <component name="addEntry">
            <file>addEntry.xml</file>
            <description>This adds new entries to ldap directory</description>
        </component>
        <component name="searchEntry">
            <file>searchEntry.xml</file>
            <description>This searches entries in ldap directory</description>
        </component>
        <component name="updateEntry">
            <file>updateEntry.xml</file>
            <description>This updates entries in ldap directory</description>
        </component>
        <component name="deleteEntry">
            <file>deleteEntry.xml</file>
            <description>This deletes entries in ldap directory</description>
        </component>
    </subComponents>
</component>