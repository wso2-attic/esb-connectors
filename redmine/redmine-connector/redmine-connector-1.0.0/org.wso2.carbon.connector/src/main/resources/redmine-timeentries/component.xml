<component name="redmine-timeentries" type="synapse/template">
    <subComponents>
        <component name="listTimeEntries">
            <file>listTimeEntries.xml</file>
            <description>Gets a list of time entries.</description>
        </component>
        <component name="getTimeEntry">
            <file>getTimeEntry.xml</file>
            <description>Gets a single time entry specified by an ID.</description>
        </component>
        <component name="createTimeEntry">
            <file>createTimeEntry.xml</file>
            <description>Creates a new time entry.</description>
        </component>
        <component name="updateTimeEntry">
            <file>updateTimeEntry.xml</file>
            <description>Updates an existing time entry.</description>
        </component>
        <component name="deleteTimeEntry">
            <file>deleteTimeEntry.xml</file>
            <description>Deletes an existing time entry.</description>
        </component>
    </subComponents>
</component>