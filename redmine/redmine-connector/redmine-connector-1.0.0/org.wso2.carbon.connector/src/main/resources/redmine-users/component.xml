<component name="redmine-users" type="synapse/template">
    <subComponents>
        <component name="listUsers">
            <file>listUsers.xml</file>
            <description>Lists existing users.</description>
        </component>
        <component name="createUser">
            <file>createUser.xml</file>
            <description>Creates a new user.</description>
        </component>
        <component name="getUser">
            <file>getUser.xml</file>
            <description>Gets details of the current user or the user specified by ID.</description>
        </component>
        <component name="updateUser">
            <file>updateUser.xml</file>
            <description>Updates details of a single user specified by an ID.</description>
        </component>
        <component name="deleteUser">
            <file>deleteUser.xml</file>
            <description>Deletes a single user.</description>
        </component>
    </subComponents>
</component>